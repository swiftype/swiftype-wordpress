{
  "name" : "swiftype/swiftype-common-php",
  "description" : "A base library used to create Swiftype PHP clients.",
  "homepage": "https://github.com/swiftype/swiftype-common-php",
  "keywords" : [
    "search",
    "client",
    "swiftype"
  ],
  "license" : "Apache-2.0",
  "type" : "library",
  "authors" : [
    {"name" : "Aur√©lien FOUCRET", "email": "aurelien.foucret@elastic.co"}
  ],
  "require" : {
    "php" : "^5.6|^7.0",
    "guzzlehttp/ringphp" : "^1.0",
    "psr/log" : "^1.0."
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5.6.0|^6.3.0",
    "squizlabs/php_codesniffer" : "^3.4.0",
    "friendsofphp/php-cs-fixer": "^2.14",
    "overtrue/phplint": "^1.1",
    "phpmd/phpmd": "^2.6"
  },
  "autoload" : {
    "psr-4" : {
      "Swiftype\\" : ""
    },
    "exclude-from-classmap" : [
      "/Tests/"
    ]
  },
  "bin": [
      "resources/dev/swiftype-codegen.sh"
  ]
}
